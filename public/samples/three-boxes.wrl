#VRML V2.0 utf8

# Three colored boxes
Transform {
  translation -3 0 0
  children [
    Shape {
      appearance Appearance {
        material Material {
          diffuseColor 1 0 0
        }
      }
      geometry Box { size 1 1 1 }
    }
  ]
}

Transform {
  translation 0 0 0
  children [
    Shape {
      appearance Appearance {
        material Material {
          diffuseColor 0 1 0
        }
      }
      geometry Box { size 1 1 1 }
    }
  ]
}

Transform {
  translation 3 0 0
  children [
    Shape {
      appearance Appearance {
        material Material {
          diffuseColor 0 0 1
        }
      }
      geometry Box { size 1 1 1 }
    }
  ]
}
